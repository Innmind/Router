{
    "name": "innmind/router",
    "type": "library",
    "description": "HTTP router",
    "keywords": ["http", "router"],
    "homepage": "http://github.com/Innmind/Router",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Router/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/url": "^2.0",
        "innmind/immutable": "^2.8",
        "innmind/http": "^3.6",
        "innmind/url-template": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Router\\": "tests/",
            "Fixture\\Innmind\\Router\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "giorgiosironi/eris": "^0.10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
