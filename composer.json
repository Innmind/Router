{
    "name": "innmind/router",
    "type": "library",
    "description": "HTTP router",
    "keywords": ["http", "router"],
    "homepage": "http://github.com/Innmind/Router",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Router/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/url": "~3.0",
        "innmind/immutable": "~3.3",
        "innmind/http": "~4.0",
        "innmind/url-template": "~2.0",
        "symfony/yaml": "^4.1|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Router\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Router\\": "tests/",
            "Fixture\\Innmind\\Router\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.16",
        "innmind/coding-standard": "^1.1"
    }
}
